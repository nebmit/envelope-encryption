{
    "version": "1.1.0",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/nebmit/envelope-encryption"
    },
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test",
        "lint": "tsdx lint --fix",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "name": "envelope-encryption",
    "author": "nebmit",
    "module": "dist/envelope-encryption.esm.js",
    "size-limit": [
        {
            "path": "dist/envelope-encryption.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/envelope-encryption.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@types/jest": "^29.2.3",
        "husky": "^8.0.2",
        "size-limit": "^8.1.0",
        "size-limit-preset-node-lib": "^0.2.0",
        "tsdx": "^0.14.1",
        "tslib": "^2.4.1",
        "typescript": "^3.9.10"
    }
}
